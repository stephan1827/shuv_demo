<?xml version="1.0" encoding="UTF-8"?>
<EventBindingSet id="ContentProfileGen_eventbinding" xmlns="http://www.br-automation.com/iat2014/eventbinding/v2"
                 xmlns:types="http://www.br-automation.com/iat2015/widgetTypes/v2"
                 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
	<Bindings>
	
		<!--Profile generator-->
		<EventBinding id="ContentProfileGen.Button1.Click">
			<Source xsi:type="widgets.brease.Button.Event" event="Click" contentRefId="ContentProfileGen" widgetRefId="Button1" />
			<EventHandler>
				<Action>
					<Target xsi:type="widgets.brease.ProfileGenerator.Action" contentRefId="ContentProfileGen" widgetRefId="ProfileGenerator1">
						<Method xsi:type="widgets.brease.ProfileGenerator.Action.EnableEditMode" itemId="ProfileGeneratorYAxisItem1"  />
					</Target>
				</Action>
			</EventHandler>
		</EventBinding>
			
		<EventBinding id="ContentProfileGen.Button3.Click">
			<Source xsi:type="widgets.brease.Button.Event" event="Click" contentRefId="ContentProfileGen" widgetRefId="Button3" />
			<EventHandler>
				<Action>
					<Target xsi:type="widgets.brease.ProfileGenerator.Action" contentRefId="ContentProfileGen" widgetRefId="ProfileGenerator1">
						<Method xsi:type="widgets.brease.ProfileGenerator.Action.EnableEditMode" itemId="ProfileGeneratorYAxisItem2"  />
					</Target>
				</Action>
			</EventHandler>
		</EventBinding>
			
		<EventBinding id="ContentProfileGen.Button2.Click">
			<Source xsi:type="widgets.brease.Button.Event" event="Click" contentRefId="ContentProfileGen" widgetRefId="Button2" />
			<EventHandler>
				<Action>
					<Target xsi:type="widgets.brease.ProfileGenerator.Action" contentRefId="ContentProfileGen" widgetRefId="ProfileGenerator1">
						<Method xsi:type="widgets.brease.ProfileGenerator.Action.DisableEditMode" />
					</Target>
				</Action>
			</EventHandler>
		</EventBinding>
  	</Bindings>
</EventBindingSet>
